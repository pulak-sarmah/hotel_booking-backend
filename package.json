{
  "name": "backend",
  "version": "1.0.0",
  "description": "node backend for hotel booking app",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "nodemon -r dotenv/config",
    "build": "npm install && npx tsc",
    "start": "node -r dotenv/config ./dist/src/index.js",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "e2e": "cross-env DOTENV_CONFIG_PATH=.env.e2e nodemon -r dotenv/config"
  },
  "keywords": [
    "node",
    "express",
    "mongoDB",
    "hotel",
    "booking"
  ],
  "author": "pulak sarmah",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.1.1",
    "nodemailer": "^6.9.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.10",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
